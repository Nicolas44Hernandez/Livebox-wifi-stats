    "version": "0.2.0",
    "configurations": [
        {
            "name": "WiFI STATS: chanim stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "chanim_stats",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "1",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
