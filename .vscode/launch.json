    "version": "0.2.0",
    "configurations": [
        {
            "name": "WiFI STATS: chanim stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "chanim_stats",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "3",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "WiFI STATS: static data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "static_livebox_data",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "WiFI STATS: info_stations_stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "stations",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "3",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "WiFI STATS: switch band 5GHz",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "switch_5GHz",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-on",
                "40",
                "-off",
                "30",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "WiFI STATS: Generate random files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "generate_random_files",
                "-f",
                "${workspaceFolder}/files_to_send",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "WiFI STATS: Files transfer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "files_transfer",
                "-fc",
                "${workspaceFolder}/wifi_stats/config/stations.yml",
                "-d",
                "2",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
    ]
}
