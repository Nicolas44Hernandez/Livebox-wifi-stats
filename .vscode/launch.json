{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GENERATE REQUESTED THROUGHPUT RESULT FILES",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/traffic_file_generation.py",
            "console": "integratedTerminal",
            "args": [
                "-tf",
                "${workspaceFolder}/wifi_stats/analysis_results/analyses_results/Campagne3/C3_SC4/2023-04-20_18-24/logs/files_transfer.log",
                "-rf",
                "${workspaceFolder}/test_file.txt",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "SETUP ANALYSIS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/setup.py",
            "console": "integratedTerminal",
            "args": [
                //"--generate-files",
                "-sc",
                "${workspaceFolder}/wifi_stats/config/stations.yml",
                "-nf", // samples per period
                "180",
                "-p", // periods
                "4",
                "-sd", // standard deviation
                "5",
                "-ss", // sample size
                "90",
                "-tc",
                "${workspaceFolder}/wifi_stats/config/traffic_config_file.yml",
                "-ti",
                "${workspaceFolder}/wifi_stats/config/plots/",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "STATIONS CALIBRATION",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/calibration.py",
            "console": "integratedTerminal",
            "args": [
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-sm",
                "6C:C7:EC:2B:2E:D7",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "LIVEBOX COUNTERTS LOG",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/log_livebox_counters.py",
            "console": "integratedTerminal",
            "args": [
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "RUN ANALYSIS: Files transfer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "files_transfer",
                "-scf",
                "${workspaceFolder}/wifi_stats/config/stations.yml",
                "-tcf",
                "${workspaceFolder}/wifi_stats/config/traffic_config_file.yml",
                "-d",
                "8",
                "-td",
                "22",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "RUN ANALYSIS: chanim stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "chanim_stats",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "1",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: static data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "static_livebox_data",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: info_stations_stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "stations",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "5",
                "-sc",
                "${workspaceFolder}/wifi_stats/config/stations_info_config.yml",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: rx_tx_stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "antenas",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "10",
                "-ac",
                "${workspaceFolder}/wifi_stats/config/antenas_tx_rx_config.yml",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sdb1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: switch band 5GHz",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "switch_5GHz",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-on",
                "40",
                "-off",
                "30",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
    ]
}
