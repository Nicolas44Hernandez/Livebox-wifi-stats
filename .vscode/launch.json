    "version": "0.2.0",
    "configurations": [
        {
            "name": "SETUP ANALYSIS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/setup.py",
            "console": "integratedTerminal",
            "args": [
                //"--generate-files",
                "-sc",
                "${workspaceFolder}/wifi_stats/config/stations.yml",
                "-fc",
                "${workspaceFolder}/wifi_stats/config/files_to_send_config.yml",
                "-sp",
                "${workspaceFolder}/wifi_stats/config/stations_profiles_config.yml",
                "-nf",
                "10",
                "-p",
                "8",
                "-tc",
                "${workspaceFolder}/wifi_stats/config/traffic_config_file.yml",
                "-ti",
                "${workspaceFolder}/wifi_stats/config/analysis_traffic",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "STATIONS CALIBRATION",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "initial_calibrate_stations",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-sm",
                "78:AF:08:31:B1:18",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
            ]
        },
        {
            "name": "RUN ANALYSIS: Files transfer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "files_transfer",
                "-scf",
                "${workspaceFolder}/wifi_stats/config/stations.yml",
                "-tcf",
                "${workspaceFolder}/wifi_stats/config/traffic_config_file.yml",
                "-d",
                "8",
                "-td",
                "22",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
            ]
        },
        {
            "name": "RUN ANALYSIS: chanim stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "chanim_stats",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "1",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: static data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "static_livebox_data",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: info_stations_stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "stations",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "5",
                "-sc",
                "${workspaceFolder}/wifi_stats/config/stations_info_config.yml",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: rx_tx_stats",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "antenas",
                "-n",
                "livebox_nico",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-sp",
                "10",
                "-ac",
                "${workspaceFolder}/wifi_stats/config/antenas_tx_rx_config.yml",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sdb1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
        {
            "name": "RUN ANALYSIS: switch band 5GHz",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wifi_stats/main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "switch_5GHz",
                "-n",
                "livebox_nico_test",
                "-l",
                "192.168.1.1",
                "-u",
                "root",
                "-pw",
                "sah",
                "-d",
                "2",
                "-on",
                "40",
                "-off",
                "30",
                "-lc",
                "${workspaceFolder}/wifi_stats/config/logging.yml",
                "-rd",
                "dev-sda1",
                "-ts",
                "2022-12-14_12-57",
            ]
        },
    ]
}
